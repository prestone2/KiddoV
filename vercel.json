{
    "version": 2,
    "builds": [{
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "src": "Public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [{
            "src": "/api/(.*)",
            "dest": "app.js"
        },
        {
            "src": "/(.*)",
            "dest": "/Public/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}